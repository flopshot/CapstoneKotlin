<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seannajera.golfiq">

    <application
        android:name=".GolfIqApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.activities.NewRoundTabActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.activities.OldRoundTabActivity" />
        <activity android:name=".presentation.activities.RangeFinderTabActivity" />
        <activity android:name=".presentation.activities.PlayerTabActivity" />
        <activity android:name=".presentation.activities.CourseTabActivity" />
    </application>

</manifest>